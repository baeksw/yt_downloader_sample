
[
  {
    "key": "f5",
    "command": "-workbench.action.debug.start"
  },
  {
    "key": "ctrl+f5",
    "command": "-workbench.action.debug.run"
  },
  {
    "key": "f5",
    "command": "python.execInTerminal",
    "when": "editorTextFocus && editorLangId == 'python'"
  },
  {
    "key": "ctrl+f5",
    "command": "workbench.action.debug.start",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+w",
    "command": "workbench.action.closeAllEditors",
    "when": "editorTextFocus"
  }
]